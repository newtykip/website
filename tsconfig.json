{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "env.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@/api/*": ["./src/pages/api/*"],
            "@/content/*": ["./content/*"]
        },
        "esModuleInterop": true
    }
}
